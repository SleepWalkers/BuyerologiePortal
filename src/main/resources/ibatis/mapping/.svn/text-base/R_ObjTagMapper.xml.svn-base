<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yunjee.common.dao.R_ObjTagDao">
  	<resultMap id="R_ObjTagMap" type="R_ObjTag" >
	    <id column="id" property="id" jdbcType="BIGINT" />
	    <result column="obj_id" property="objId" jdbcType="BIGINT" />
	    <result column="type" property="type" jdbcType="VARCHAR" />
	    <result column="tag_id" property="tagId" jdbcType="BIGINT" />
	    <result column="weight" property="weight" jdbcType="DOUBLE" />
  	</resultMap>
  	
	<insert id="insertR_ObjTag" parameterType="R_ObjTag">
		INSERT INTO yunjee_r_obj_tag
	      (obj_id, type, tag_id, weight)
	    VALUES
	      (#{objId}, #{type}, #{tagId}, #{weight})
	</insert>
	
	<update id="updateR_ObjTag" parameterType="R_ObjTag">
    	UPDATE yunjee_r_obj_tag 
    	<set>
    		<if test="objId != null">
    			obj_id = #{objId},
	        </if>
    		<if test="type != null">
    			type = #{type},
	        </if>
      		<if test="tagId != null">
    			tag_id = #{tagId},
	        </if>
	        <if test="weight != null">
    			weight = #{weight}
	        </if>
	    </set>
    	WHERE id = #{id}
    </update>
    
    <delete id="deleteById" parameterType="int">
    	DELETE FROM yunjee_r_obj_tag where id = #{id}
    </delete>
    
    <delete id="deleteByR_ObjTag" parameterType="R_ObjTag">
    	DELETE
    	FROM yunjee_r_obj_tag
    	<trim prefix="WHERE" prefixOverrides="AND|OR">
    		<if test="objId != null">
    			obj_id = #{objId}
	        </if>
    		<if test="type != null">
    			AND type = #{type}
	        </if>
      		<if test="tagId != null">
    			AND tag_id = #{tagId}
	        </if>   	
	        <if test="weight != null">
    			weight = #{weight}
	        </if>
	        	OR ID = 0
    	</trim>
    </delete>
    
    <select id="selectById" parameterType="int" resultMap="R_ObjTagMap">
    	SELECT * FROM yunjee_r_obj_tag WHERE id = #{id}
    </select>
    
    <select id="selectByR_ObjTag" parameterType="R_ObjTag" resultMap="R_ObjTagMap">
    	SELECT *
    	FROM yunjee_r_obj_tag
    	<trim prefix="WHERE" prefixOverrides="AND|OR">
    		<if test="objId != null">
    			obj_id = #{objId}
	        </if>
    		<if test="type != null">
    			AND type = #{type}
	        </if>
      		<if test="tagId != null">
    			AND tag_id = #{tagId}
	        </if>
	        <if test="weight != null">
    			weight = #{weight}
	        </if>
    	</trim>    	
    </select>
</mapper>